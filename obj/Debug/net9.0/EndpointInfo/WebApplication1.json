{
  "openapi": "3.0.4",
  "info": {
    "title": "WebApplication1",
    "version": "1.0"
  },
  "paths": {
    "/api/Dictionary/generate": {
      "post": {
        "tags": [
          "Dictionary"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DictionarySchema"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/DictionarySchema"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/DictionarySchema"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Dictionary/{className}/add-field": {
      "post": {
        "tags": [
          "Dictionary"
        ],
        "parameters": [
          {
            "name": "className",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FieldSchema"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/FieldSchema"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/FieldSchema"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Dictionary/types": {
      "get": {
        "tags": [
          "Dictionary"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Dictionary/dynamic-types": {
      "get": {
        "tags": [
          "Dictionary"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "DictionarySchema": {
        "required": [
          "name"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true,
            "example": "test"
          }
        },
        "additionalProperties": false
      },
      "FieldSchema": {
        "required": [
          "name",
          "type"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "type": {
            "$ref": "#/components/schemas/FieldType"
          },
          "referenceType": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "FieldType": {
        "enum": [
          0,
          1,
          2,
          3,
          4,
          5
        ],
        "type": "integer",
        "format": "int32"
      }
    }
  }
}